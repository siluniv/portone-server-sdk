{
  "name": "@siluniv/portone-server-sdk",
  "type": "module",
  "version": "0.1.2",
  "description": "Fork of PortOne JavaScript SDK for server-side usage (adds Idempotency-Key support)",
  "keywords": [
    "PortOne",
    "payment",
    "포트원",
    "결제 연동"
  ],
  "author": {
    "name": "siluniv"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siluniv/portone-server-sdk.git",
    "directory": "javascript"
  },
  "homepage": "https://github.com/siluniv/portone-server-sdk#readme",
  "bugs": {
    "url": "https://github.com/siluniv/portone-server-sdk/issues"
