    "./platform/transfer": {
      "types": "./dist/generated/platform/transfer/index.d.ts",
      "require": "./dist/generated/platform/transfer/index.cjs",
      "import": "./dist/generated/platform/transfer/index.mjs"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "format": "biome format --write .",
    "build": "unbuild",
    "test:dev": "vitest --ui --typecheck",
    "test": "vitest run --typecheck",
    "prepublishOnly": "npm run build",
    "publish:jsr": "jsr publish --allow-dirty"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@changesets/cli": "^2.29.7",
    "@expo/spawn-async": "^1.7.2",
    "@types/node": "^24.10.1",
    "@vitest/ui": "^3.1.4",
    "jsr": "^0.13.5",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^3.1.4"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "license": "(Apache-2.0 OR MIT)",
